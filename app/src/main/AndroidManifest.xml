<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akv.newsiebuild">

    <application
        android:allowBackup="true"
        android:icon="@drawable/news_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewsieBuild">
        <activity
            android:name="com.akv.newsiebuild.activity.ProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.akv.newsiebuild.activity.ReadLaterActivity"
            android:exported="false" />
        <activity
            android:name="com.akv.newsiebuild.activity.DetailNewsActivity"
            android:exported="false" />
        <activity
            android:name="com.akv.newsiebuild.activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.akv.newsiebuild.activity.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.akv.newsiebuild.util.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>